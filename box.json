{
    "output": "symfony-certification.phar",
    "main": "bin/exam-start",
    "directories": [
        "src",
        "data"
    ],
    "files": [
        "composer.json"
    ],
    "finder": [
        {
            "name": "*.php",
            "exclude": [
                "tests"
            ],
            "in": "vendor"
        }
    ],
    "compression": "GZ",
    "check-requirements": false
}
